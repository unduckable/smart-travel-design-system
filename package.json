{
  "name": "smart-travel-ds",
  "version": "0.0.1",
  "description": "Smart Travel Design System",
  "author": {
    "name": "Anh Le",
    "email": "ledzanh@gmail.com"
  },
  "contributors": [
    {
      "name": "Hieu DN"
    }
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.es.js",
      "default": "./dist/index.umd.js"
    },
    "./dist/": "./dist/",
    "./types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "----------BUILD----------": "",
    "build": "vite build --config configs/build.js",
    "build:storybook": "storybook build",
    "----------DEV----------": "",
    "dev": "vite --config configs/dev.js",
    "dev:storybook": "storybook dev -p 6006",
    "----------TEST----------": "",
    "test": "vitest --config configs/test.js",
    "----------OTHER----------": "",
    "generate-icons": "node scripts/generateJSXIcon.js",
    "check": "@biomejs/biome check --apply .",
    "clean": "rm -rf dist storybook-static",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "nano-staged": {
    "*.{css,json,md,yaml,yml,js,ts,tsx}": "npx @biomejs/biome check --apply"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^10.16.5",
    "react-aria": "^3.29.1",
    "react-aria-components": "^1.0.0-rc.0",
    "swiper": "^11.0.5",
    "tailwindcss-react-aria-components": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.1",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@storybook/addon-a11y": "^7.5.2",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-storysource": "^7.5.2",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@swc/core": "^1.3.96",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "storybook-addon-swc": "^1.2.0",
    "tailwindcss": "^3.3.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-commonjs": "^0.10.1",
    "vite-plugin-dts": "^3.6.1",
    "vitest": "^0.34.6",
    "wouter": "^2.12.1"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
