{
  "name": "smart-travel-ds",
  "version": "0.0.1",
  "description": "Smart Travel Design System",
  "license": "ISC",
  "author": {
    "name": "Anh Le",
    "email": "ledzanh@gmail.com"
  },
  "contributors": [
    {
      "name": "Hieu DN"
    }
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.es.js",
      "default": "./dist/index.umd.js"
    },
    "./dist/": "./dist/",
    "./types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build --config config/build.js",
    "build:storybook": "storybook build",
    "check": "@biomejs/biome check --apply .",
    "clean": "rm -rf dist storybook-static",
    "dev": "vite --config config/dev.js",
    "dev:storybook": "storybook dev -p 6006",
    "prepare": "husky install",
    "test": "vitest --config config/test.js"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "nano-staged": {
    "*.{css,json,md,yaml,yml,js,ts,tsx}": "npx @biomejs/biome check --apply",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "react-aria": "^3.29.1",
    "react-aria-components": "^1.0.0-beta.2",
    "tailwindcss-react-aria-components": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.1",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-package-json": "^2.6.0",
    "storybook": "^7.5.1",
    "tailwindcss": "^3.3.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.1",
    "vitest": "^0.34.6",
    "wouter": "^2.12.1"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
